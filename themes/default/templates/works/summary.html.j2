<p class="meta">Published on {{ date }} by <b>{{ author|join(', ') }}</b></p>
{% if series %} {% for s in series %} Part {{s.index}} of {{s.title}}{% if not loop.last %}, {% endif %}{% endfor %} {% endif %}
<p class="fandom">Fandom: {{ fandom|join(', ') }}</p>
<p class="summary">
{{ summary }}
</p>
